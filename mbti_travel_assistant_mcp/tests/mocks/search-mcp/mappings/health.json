{
  "request": {
    "method": "GET",
    "urlPath": "/health"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "status": "healthy",
      "service": "restaurant-search-mcp-mock",
      "version": "1.0.0",
      "timestamp": "{{now format='yyyy-MM-dd HH:mm:ss'}}",
      "uptime": "{{now format='PT'}}",
      "checks": {
        "database": "healthy",
        "memory": "healthy",
        "disk": "healthy"
      }
    }
  }
}